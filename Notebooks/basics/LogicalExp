println(!true)
println(!false)
println(true && true)
println(false && false)
println(true && false)
println(false && false)
println(true || true)
println(false || true)
println(true || false)
println(false || false)
println(1 < 22)
println(-10 > 3)
println(2 == 4-2)
println(( 3-4 > 1 ) && ( 2+2 == 4 ) || ( 10-5 > 2 ))
println(false  && true || true)
